    "test:lint:fix": "eslint . --fix 'src/**/*.ts'"
  },
  "dependencies": {
    "axios": "^1.6.4",
    "body-parser": "^1.20.2",
    "compression": "1.7.4",
    "express": "^4.19.2",
    "moment": "^2.29.4",
    "node-fetch": "^3.3.2",
    "ts-node": "10.9.2",
    "typescript": "5.2.2",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.18",
    "@types/helmet": "4.0.0",
    "@types/jest": "28.1.8",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.61.0",
    "codecov": "3.8.3",
    "dotenv": "^16.3.1",
    "eslint": "8.50.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "prettier": "2.8.8",
    "ts-jest": "28.0.8"
  }
}
